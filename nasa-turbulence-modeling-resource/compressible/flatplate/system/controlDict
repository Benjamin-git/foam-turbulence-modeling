FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoSimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         150000;

deltaT          1;

writeControl    timeStep;

writeInterval   50;

purgeWrite      5;

writeFormat     binary;

writePrecision  15;

writeCompression on;

timeFormat      general;

timePrecision   12;

graphFormat     raw;

runTimeModifiable true;


